{
  "name": "angularjs",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "private": true,
  "scripts": {
    "lint": "eslint app --config=config/eslint/.eslintrc",
    "start": "better-npm-run server",
    "build:prod": "better-npm-run build:prod",
    "test": "better-npm-run test",
    "analyze": "better-npm-run analyze"
  },
  "betterScripts": {
    "server": {
      "command": "node server.js",
      "env": {
        "NODE_ENV": "development",
        "PORT": 8080
      }
    },
    "build:prod": {
      "command": "webpack --config --colors",
      "env": {
        "NODE_ENV": "production",
        "PORT": 8080
      }
    },
    "test": {
      "command": "karma start ./config/karma/karma.conf.js",
      "env": {
        "NODE_ENV": "test",
        "PORT": 8080
      }
    },
    "analyze": {
      "command": "mkdirp reports && webpack --json -profile > reports/webpack-stats.json",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jayser/angularjs.git"
  },
  "bugs": {
    "url": "https://github.com/Jayser/angularjs/issues"
  },
  "homepage": "https://github.com/Jayser/angularjs#readme",
  "devDependencies": {
    "angular": "1.5.9",
    "angular-ui-router": "1.0.0-beta.3",
    "autoprefixer": "6.5.3",
    "babel-core": "6.18.2",
    "babel-loader": "6.2.8",
    "babel-plugin-transform-runtime": "6.15.0",
    "babel-preset-latest": "6.16.0",
    "babel-preset-stage-0": "6.16.0",
    "babel-runtime": "6.18.0",
    "better-npm-run": "0.0.13",
    "clean-webpack-plugin": "0.1.14",
    "css-loader": "0.26.0",
    "eslint": "3.10.2",
    "eslint-config-angular": "0.5.0",
    "eslint-loader": "1.6.1",
    "eslint-plugin-angular": "1.4.1",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "force-case-sensitivity-webpack-plugin": "0.2.1",
    "html-webpack-plugin": "2.24.1",
    "jasmine-core": "2.5.2",
    "karma": "1.3.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-coverage": "1.1.1",
    "karma-jasmine": "1.0.2",
    "karma-phantomjs-launcher": "1.0.2",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "1.8.0",
    "ng-annotate-loader": "0.2.0",
    "postcss-loader": "1.1.1",
    "precss": "1.4.0",
    "pug": "2.0.0-beta6",
    "pug-loader": "2.3.0",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "1.13.3",
    "webpack-dev-server": "1.16.2",
    "webpack-merge": "0.17.0",
    "webpack-validator": "2.2.9"
  },
  "dependencies": {
    "font-awesome": "4.7.0"
  }
}
