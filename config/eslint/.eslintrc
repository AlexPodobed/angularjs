{
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 7,
    "sourceType": "module",
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  },
  "plugins": ["angular"],
  "extends": ["eslint:recommended", "angular"],
  "rules": {
    "no-console": "off",
    "no-debugger": "off"
  }
}